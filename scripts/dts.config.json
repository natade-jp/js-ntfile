{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.m?js$",
		"excludePattern": "\\.test\\.m?js$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"referenceTitle": "My SDK Name",
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"template": "./node_modules/@otris/jsdoc-tsd",
		"destination": "./docs",
		"encoding": "utf8",
		"recurse": true
	}
}
